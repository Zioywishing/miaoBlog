{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~": ["./app"],
      "~/*": ["./app/*"],
      "@": ["./app"],
      "@/*": ["./app/*"],
      "~~": ["."],
      "~~/*": ["./*"],
      "@@": ["."],
      "@@/*": ["./*"]
    }
  },
  "include": [
    "app/**/*",
    "server/**/*",
    ".nuxt/imports.d.ts",
    ".nuxt/nuxt.d.ts"
  ],
  "exclude": [
    ".nuxt/components.d.ts"
  ]
}
